#include <stdio.h>

int main()
{
   int a[5],i,j,flag;
   printf("Enter the elements:\n");
   for(i=0;i<=4;i++)
   {
       scanf(" %d", &a[i]);
   }
   printf("The Elements are:\n");
   for(i=0;i<5;i++)
   {
       printf(" %d ",a[i]);
   }
   printf("\nThe Prime numbers are:\n");
   for(i=0;i<5;i++)
   {
       flag=0;
       for(j=2;j<a[i];j++)
       {
           if(a[i]%j==0)
           {
               flag=1;
               break;
           }
       }
       if(flag==0)
       {
       if(a[i]!=1&&a[i]!=0)
       printf("\n %d ",a[i]);
       }
   }
    return 0;
}
