int a[20];
    int i,n,sum,k;
    printf("enter n");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    sum=0;
    for(i=0;i<n;i++)
    { 
       int f=1;
          k= a[i];
        while(k>0)
        {
       //k= a[i];
        f=f*k;
        k--;
        }
     sum=sum+f;
    }
   printf("Sum of factorial of values is %d",sum);
    return 0;
}
